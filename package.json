{
  "name": "soybean-admin-vue2",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "dev:prod": "vite --mode production",
    "build": "yarn tsCheck  && vite build",
    "build:dev": "yarn tsCheck && vite build --mode development",
    "serve": "vite preview",
		"tsCheck":"vue-tsc --noEmit --skipLibCheck",
    "lint": "eslint ./src --ext .vue,.js,jsx,.ts,tsx",
    "lint:fix": "eslint --fix ./src --ext .vue,.js,jsx,.ts,tsx",
    "prepare": "husky install"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "lint-staged": {
    "*.{vue,js,jsx,ts,tsx}": "eslint --fix"
  },
  "dependencies": {
    "@vue/composition-api": "^1.4.6",
    "pinia": "^2.0.11",
    "vue": "^2.6.14",
    "vue-demi": "^0.12.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/chroma-js": "^2.1.3",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/runtime-dom": "^3.2.31",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.3.0",
    "eslint": "^8.9.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.4.1",
    "husky": "^7.0.4",
    "less": "^4.1.2",
    "lint-staged": "^12.3.4",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.5.1",
    "rollup-plugin-visualizer": "^5.5.4",
    "sass": "^1.49.8",
    "typescript": "^4.5.5",
    "unplugin-icons": "^0.13.2",
    "unplugin-vue-components": "^0.17.18",
    "unplugin-vue2-script-setup": "^0.9.3",
    "vite": "^2.8.4",
    "vite-plugin-html": "^3.0.6",
    "vite-plugin-vue2": "^1.9.3",
    "vite-plugin-windicss": "^1.7.1",
    "vue-template-compiler": "^2.6.14",
    "vue-tsc": "^0.31.4",
    "windicss": "^3.4.3"
  }
}
